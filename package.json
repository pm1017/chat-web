{
  "name": "chat",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "prepare": "husky"
  },
  "keywords": [
    "vue",
    "vite",
    "element-plus",
    "pinia",
    "axios"
  ],
  "author": "pm1017",
  "license": "MIT",
  "description": "A chat application built with Vue 3, Vite, and Element Plus",
  "repository": {
    "type": "git",
    "url": "https://github.com/pm1017/chat.git"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "^1.9.0",
    "element-plus": "^2.10.1",
    "normalize.css": "^8.0.1",
    "pinia": "^3.0.3",
    "vue": "^3.5.16",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.15.30",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.28.0",
    "eslint-plugin-vue": "~10.0.1",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "lint-staged": "^16.1.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "3.5.3",
    "sass": "^1.89.2",
    "typescript": "~5.8.3",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.7.0",
    "vite": "^6.3.5",
    "vite-plugin-vue-devtools": "^7.7.6",
    "vue-tsc": "^2.2.10"
  },
  "lint-staged": {
    "src/**/*.{js,ts,vue}": [
      "eslint --fix"
    ],
    "src/**/*.{js,ts,vue,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
